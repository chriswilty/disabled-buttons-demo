{
	"extends": ["@tsconfig/strictest/tsconfig.json"],
	"compilerOptions": {
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"target": "ES2022",
		"jsx": "react-jsx",
		"allowImportingTsExtensions": true,
		"noImplicitAny": true,
		"sourceMap": true
	},
	"include": ["src", "vite.config.ts", ".eslintrc.cjs"]
}
